<section class="products_list">
    <h1>Lista de productos:</h1>
    <table>
        <tr class="columnas">
            <th>Producto</th>
            <th>Precio</th>
            <th>Stock</th>
        </tr>
        
        {{#each products}}
            <tr>
                <td>{{this.title}}</td>
                    <td>{{this.price}}</td>
                    <td>{{this.stock}}</td>
            </tr>
        {{/each}}
    </table>
</section>